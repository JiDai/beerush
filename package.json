{
  "name": "beerush",
  "version": "1.0.0",
  "description": "BeeRush, the official game",
  "repository": "https://github.com/JiDai/beerush",
  "main": "src/index.js",
  "directories": {
    "test": "tests"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "start": "WEBPACK_HOST=${WEBPACK_HOST:-localhost} WEBPACK_PORT=${WEBPACK_PORT:-8080} node ./build-tools/webpack.server.js",
    "lint": "./node_modules/.bin/eslint --ext .jsx,.js ./src",
    "lint:fix": "./node_modules/.bin/eslint --ext .jsx,.js --fix ./src",
    "test": "./node_modules/.bin/mocha --opts ./tests/mocha.opts tests/**/*.test.js* --compilers js:babel-core/register",
    "test:watch": "./node_modules/.bin/mocha --opts ./tests/mocha.opts --watch tests/**/*.test.js*",
    "test:file": "./node_modules/.bin/mocha --opts ./tests/mocha.opts",
    "test:file:watch": "./node_modules/.bin/mocha --opts ./tests/mocha.opts --watch",
    "cover": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- --opts ./tests/mocha.opts ./tests/unit",
    "cover:file": " ./node_modules/.bin/istanbul ./node_modules/.bin/babel-istanbul cover _mocha -- --opts ./tests/mocha.opts",
    "cover:all": "./node_modules/.bin/istanbul cover --include-all-sources _mocha -- --opts ./tests/mocha.opts ./tests/unit"
  },
  "devDependencies": {
    "babel-cli": "6.22.2",
    "babel-core": "6.22.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.10",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-rewire": "1.0.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.22.0",
    "babel-preset-stage-0": "6.22.0",
    "chai": "3.5.0",
    "enzyme": "2.7.1",
    "eslint": "3.14.1",
    "eslint-config-standard": "6.2.1",
    "eslint-config-standard-react": "4.2.0",
    "eslint-loader": "1.6.1",
    "eslint-plugin-babel": "4.0.1",
    "eslint-plugin-promise": "3.4.0",
    "eslint-plugin-react": "6.9.0",
    "eslint-plugin-standard": "2.0.1",
    "istanbul": "1.1.0-alpha.1",
    "jsdom": "9.9.1",
    "mocha": "3.2.0",
    "react-addons-test-utils": "15.4.2",
    "react-hot-loader": "3.0.0-beta.6",
    "sass-loader": "4.1.1",
    "sinon": "1.17.7",
    "sinon-chai": "2.8.0",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.2.0",
    "webpack-dotenv-plugin": "1.4.0"
  },
  "dependencies": {
    "classnames": "2.2.5",
    "css-loader": "0.26.1",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-redux": "5.0.2",
    "react-router": "3.0.2",
    "react-router-redux": "4.0.7",
    "redux": "3.6.0",
    "redux-thunk": "2.2.0"
  },
  "author": "",
  "license": "ISC"
}
